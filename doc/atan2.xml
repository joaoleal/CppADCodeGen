<mo stretchy="false">/</mo>
<msqrt><mrow><msup><mi mathvariant='italic'>x</mi>
<mn>2</mn>
</msup>
<mo stretchy="false">+</mo>
<msup><mi mathvariant='italic'>y</mi>
<mn>2</mn>
</msup>
</mrow>
