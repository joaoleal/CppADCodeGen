# include &lt;cmath&gt;           // define fabs function
# include &quot;exp_2.hpp&quot;       // definition of exp_2 algorithm
bool exp_2(void)
{	double x     = .5;
	double check = 1 + x + x * x / 2.; 
	bool   ok    = std::fabs( exp_2(x) - check ) &lt;= 1e-10; 
	return ok;
}</pre>
